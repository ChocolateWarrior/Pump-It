<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pumpit.app">

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@mipmap/pump_it_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/pump_it_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.view.activity.registration.ClientSecondStepRegistrationActivity" />
        <activity android:name=".ui.view.activity.registration.TrainerSecondStepRegistrationActivity" />
        <activity android:name=".ui.view.activity.registration.FirstStepRegistrationActivity" />
        <activity android:name=".ui.view.activity.registration.LoginActivity"/>
        <activity android:name=".ui.view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>